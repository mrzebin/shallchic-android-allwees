<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@color/app_background_default"
    android:clipChildren="false"
    >
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        >
        <LinearLayout
            android:id="@+id/ll_inflateTp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            >
        </LinearLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/item_pspace40"
            android:paddingStart="@dimen/item_pspace10"
            android:paddingEnd="@dimen/item_pspace10"
            >
            <ImageView
                android:id="@+id/iv_back"
                android:layout_width="20dp"
                android:layout_height="20dp"
                android:layout_centerVertical="true"
                android:src="@mipmap/ic_black_back"
                >
            </ImageView>
            <TextView
                android:id="@+id/tv_topTitle"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="@dimen/app_text_16sp"
                android:layout_centerInParent="true"
                android:text="@string/deliver_double_title"
                >
            </TextView>
            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/cl_comeMyCart"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_alignParentEnd="true"
                android:layout_centerVertical="true"
                >
                <ImageView
                    android:id="@+id/iv_shipCart_fs"
                    android:layout_width="@dimen/item_pspace20"
                    android:layout_height="@dimen/item_pspace20"
                    android:src="@mipmap/ic_shopping_cart_1"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintBottom_toBottomOf="parent"
                    >
                </ImageView>
                <TextView
                    android:id="@+id/tv_buyNum"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text=""
                    android:textSize="@dimen/app_text_word11"
                    android:textColor="@color/color_333"
                    app:layout_constraintStart_toStartOf="@id/iv_shipCart_fs"
                    app:layout_constraintEnd_toEndOf="@id/iv_shipCart_fs"
                    app:layout_constraintTop_toTopOf="parent"
                    >
                </TextView>
            </androidx.constraintlayout.widget.ConstraintLayout>

            <View
                android:id="@+id/v_hasCart"
                android:layout_width="@dimen/item_pspace5"
                android:layout_height="@dimen/item_pspace5"
                android:background="@drawable/shape_red_5_indicate"
                android:layout_gravity="center_horizontal"
                android:visibility="gone"
                android:layout_toStartOf="@id/cl_comeMyCart"
                android:layout_marginTop="@dimen/item_pspace10"
                >
            </View>
        </RelativeLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="@dimen/item_pspace1"
            android:background="@color/app_line_color"
            >
        </View>

        <androidx.swiperefreshlayout.widget.SwipeRefreshLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/srl_deliverDouble"
            >
            <com.hb.basemodel.utils.RecycerLoadMoreScrollView
                android:id="@+id/rlv_moreDeliverDouble"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:fillViewport="true"
                android:nestedScrollingEnabled="false"
                android:scrollbars="none"
                >
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical"
                    >
                    <com.qmuiteam.qmui.widget.QMUIRadiusImageView
                        android:id="@+id/iv_activeDeliverTow"
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/item_pspace115"
                        android:scaleType="fitXY"
                        android:visibility="gone"
                        android:layout_marginTop="@dimen/item_pspace10"
                        android:layout_marginStart="@dimen/item_pspace10"
                        android:layout_marginEnd="@dimen/item_pspace10"
                        app:qmui_corner_radius="@dimen/item_pspace8"
                        app:qmui_border_width="0dp"
                        >
                    </com.qmuiteam.qmui.widget.QMUIRadiusImageView>
                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:descendantFocusability="blocksDescendants"
                        android:orientation="vertical"
                        android:visibility="visible"
                        android:layout_marginTop="@dimen/item_pspace10"
                        android:paddingStart="@dimen/item_pspace5"
                        android:paddingEnd="@dimen/item_pspace5"
                        >
                        <androidx.recyclerview.widget.RecyclerView
                            android:id="@+id/rv_actionGetDoubleGoods"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:visibility="visible"
                            >
                        </androidx.recyclerview.widget.RecyclerView>
                    </RelativeLayout>
                    <include layout="@layout/layout_no_more" />
                </LinearLayout>
            </com.hb.basemodel.utils.RecycerLoadMoreScrollView>
        </androidx.swiperefreshlayout.widget.SwipeRefreshLayout>
    </LinearLayout>

</LinearLayout>