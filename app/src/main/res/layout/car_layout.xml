                                    android:id="@+id/qrb_applyPromo"
                                    android:layout_width="wrap_content"
                                    android:layout_height="match_parent"
                                    android:text="@string/str_apply"
                                    android:textSize="@dimen/app_text_word12"
                                    android:textColor="@color/color_30c0ff"
                                    android:paddingTop="@dimen/item_pspace5"
                                    android:paddingBottom="@dimen/item_pspace5"
                                    android:paddingStart="@dimen/item_pspace20"
                                    android:paddingEnd="@dimen/item_pspace20"
                                    app:qmui_radius="@dimen/item_pspace5"
                                    app:qmui_borderColor="@color/color_30c0ff"
                                    app:qmui_borderWidth="@dimen/item_pspace0.5"
                                    android:layout_marginStart="@dimen/item_pspace5"
                                    >
                                </com.qmuiteam.qmui.widget.roundwidget.QMUIRoundButton>
                            </LinearLayout>
                            <TextView
                                android:id="@+id/tv_invalidCouponCode"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="@string/cart_view_no_csah"
                                android:textSize="@dimen/item_pspace12"
                                android:textColor="@color/color_ff4040"
                                android:paddingTop="@dimen/item_pspace10"
                                android:paddingStart="@dimen/item_pspace10"
                                android:paddingEnd="@dimen/item_pspace10"
                                android:visibility="gone"
                                >
                            </TextView>
                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:background="@color/white"
                                android:orientation="horizontal"
                                android:padding="@dimen/item_pspace10"
                                >
                                <TextView
                                    android:id="@+id/tv_choiceCouponCode"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="@string/cart_view_avaliable_c"
                                    android:textSize="@dimen/item_pspace12"
                                    android:textColor="@color/allwees_theme_color"
                                    >
                                </TextView>
                                <TextView
                                    android:id="@+id/tv_rightArrow"
                                    android:layout_width="wrap_content"
