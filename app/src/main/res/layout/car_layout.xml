                                        android:layout_height="wrap_content"
                                        android:text="@string/str_nomore"
                                        android:textSize="@dimen/app_text_word12"
                                        android:textColor="@color/color_999"
                                        android:paddingStart="@dimen/item_pspace20"
                                        android:paddingEnd="@dimen/item_pspace20"
                                        >
                                    </TextView>
                                    <View
                                        android:layout_width="@dimen/item_pspace30"
                                        android:layout_height="1dp"
                                        android:background="@color/color_999"
                                        android:layout_gravity="center_vertical"
                                        >
                                    </View>
                                </LinearLayout>
                            </LinearLayout>
                        </LinearLayout>
