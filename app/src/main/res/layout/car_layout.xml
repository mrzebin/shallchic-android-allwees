                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="vertical"
                                android:background="@color/app_background_default"
                                android:paddingTop="@dimen/item_pspace10"
                                >
                                <LinearLayout
                                    android:id="@+id/ll_cIncreaseG"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:orientation="horizontal"
                                    android:background="@color/color_fff4d"
                                    android:padding="@dimen/item_pspace10"
                                    >
                                    <TextView
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:text="@string/str_upper_add"
